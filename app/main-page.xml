<Page xmlns="http://schemas.nativescript.org/tns.xsd" navigatingTo="navigatingTo" class="page">
  <Page.actionBar>
    <ActionBar title="OAuth Demo: TypeScript" icon="" class="action-bar">
    </ActionBar>
  </Page.actionBar>

  <StackLayout class="p-20">
    <Button text="Login facebook" tap="onLoginTap" class="btn btn-primary btn-active"/>
    <Button text="Login google" tap="onLoginTapg" class="btn btn-primary btn-active"/>
    <Button text="Login twitter" tap="onLoginTapTwit" class="btn btn-primary btn-active"/>
    <Button text="Logout" tap="onLogoutTap" class="btn btn-primary btn-active"/>
    <Label id="myLabel" text="{{ myText }}" />
  </StackLayout>
</Page>